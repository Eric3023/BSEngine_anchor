.container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.header {
  height: 196rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 10rpx;
  background-color: white;
  box-shadow: 0 0 4rpx #dedede;
}

.img {
  width: 132rpx;
  height: 132rpx;
  border-radius: 5rpx;
  margin: 0 28rpx;
}

.column {
  width: 536rpx;
  display: flex;
  flex-direction: column;
}

.name {
  width: 536rpx;
}

.status {
  color: #ffbb00;
  margin-top: 12rpx;
}

.lines {
  width: 100%;
  height: 190rpx;
  margin-top: 28rpx;
}

.background {
  width: 100%;
  height: 100%;
}

.content {
  display: flex;
  flex-direction: column;
  width: 680rpx;
  margin-top: 28rpx;
}

.item {
  display: flex;
  flex-direction: row;
}

.time {
  width: 152rpx;
}

.icon {
  width: 38rpx;
}

.desc {
  width: 474rpx;
  margin-left: 16rpx;
  font-size: 30rpx;
}

.second{
  font-size: 26rpx;
}

.date{
  font-size: 20rpx;
  margin-top: 12rpx;
}

.upper{
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
}

.line{
  height: 105rpx;
  width: 2rpx;
  background-color: #d6d6d6;
}

.foucus{
  color: black;
}

.noFocus{
  color: #848484;
}