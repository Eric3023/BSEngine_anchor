page {
  background-color: #f9fcfc;
}

.feed{
  max-width: 454rpx;
}

.container {
  align-items: center;
}

.img{
  width: 640rpx;
  margin-top: 10rpx;
}

.mediaIcon {
  width: 64rpx;
  height: 64rpx;
  border-radius: 10rpx;
}

.txt {
  min-width: 640rpx;
  display: flex;
  flex-direction: column;
  margin: 10rpx 50rpx 28rpx 50rpx;
  padding: 0 10rpx 120rpx 10rpx;
  border: 2rpx #ffbb00 solid;
  font-size: 28rpx;
}

.txt_container{
  display: flex;
  flex-direction: column;
  padding: 0 10rpx;
}

.icon {
  margin-top: 28rpx;
  width: 172rpx;
  height: 172rpx;
}

.box {
  border: 1rpx #ffbb00 dashed;
  padding: 6rpx 10rpx;
}

.title {
  margin-top: 32rpx;
  color: #000000;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.desc {
  margin-top: 72rpx;
  color: #000000;
  display: flex;
  flex-direction: column;
}

.desc .desc_box {
  margin-top: 18rpx;
  min-height: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}

.time {
  margin-top: 50rpx;
  color: #000000;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.end,
.category {
  margin-top: 72rpx;
  color: #000000;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.like_container{
  width: 640rpx;
  display: flex;
  margin-top: 12rpx;
  flex-direction: row-reverse;
  align-items: center;
}

.like_container image{
  width: 58rpx;
  height: 58rpx;
  margin-right: 8rpx;
}

.like_container text{
  font-size: 28rpx;
}