var numberFormat = function (number, count) {
  if (number)
    return number.toFixed(count);
  else
    return '0.00';
}

var substring = function (str, number) {
  if (number < str.length) {
    str = str.substring(str.length - number)
  }
  return str
}

module.exports = {
  numberFormat: numberFormat,
  substring: substring,
}

